{
  "type": "AdaptiveCard",
  "body": [
    {
      "type": "TextBlock",
      "size": "Medium",
      "weight": "Bolder",
      "text": "Wazuh Agent Health and Status"
    },
    {
      "type": "FactSet",
      "facts": [
        {
          "title": "Agent ID:",
          "value": "${agentId}"
        },
        {
          "title": "Agent Name:",
          "value": "${agentName}"
        },
        {
          "title": "Last Check-in:",
          "value": "${lastCheckin}"
        },
        {
          "title": "Status:",
          "value": "${status}"
        },
        {
          "title": "OS:",
          "value": "${operatingSystem}"
        },
        {
          "title": "IP Address:",
          "value": "${ipAddress}"
        },
        {
          "title": "Active Rules:",
          "value": "${activeRules}"
        }
      ]
    }
  ],
  "$schema": "http://adaptivecards.io/schemas/adaptive-card.json",
  "version": "1.4"
}